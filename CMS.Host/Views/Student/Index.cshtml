@model List<CMS.Business.Dtos.StudentDto>
@{
    ViewBag.Title = "Students";
}


<div class="page-header">
    <h1>Students</h1>
    @Html.ActionLink("Create", $"Edit", null, new { @class = "btn btn-info" })
</div>

<div>
    <table  class="table table-bordered table-condensed table-responsive table-hover table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Registration No.</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var s in Model) {
            <tr>
                <td>@s.Name</td>
                <td>@s.Addmitions.First().Id</td>
            </tr>
            }
        </tbody>
    </table>
</div>